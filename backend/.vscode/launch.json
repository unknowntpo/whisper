{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "FastAPI",
			"type": "debugpy",
			"request": "launch",
			"module": "uvicorn",
			"args": [
				"main:app",
				"--reload",
				"--host",
				"0.0.0.0",
				"--port",
				"8000"
			],
			"jinja": true,
			"justMyCode": true,
			"env": {
				"PYTHONPATH": "${workspaceFolder}"
			}
		}
	]
}